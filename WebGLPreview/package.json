{
  "name": "world-architect",
  "type": "module",
  "scripts": {
    "dev": "parcel src/index.html --open",
    "build": "parcel build src/index.html --no-source-maps --public-url https://n5ednd.web.app --no-cache",
    "clean": "npm run clean-windows || npm run clean-linux",
    "clean-linux": "rm -rf ./.cache && rm -rf ./dist",
    "clean-windows": "rmdir /Q/S ./.cache & rmdir /Q/S ./dist"
  },
  "overrides": {
    "lmdb": "2.6.0-alpha6",
    "@lmdb/lmdb-darwin-arm64": "2.6.0-alpha6",
    "@lmdb/lmdb-darwin-x64": "2.6.0-alpha6",
    "@lmdb/lmdb-linux-arm": "2.6.0-alpha6",
    "@lmdb/lmdb-linux-arm64": "2.6.0-alpha6",
    "@lmdb/lmdb-linux-x64": "2.6.0-alpha6",
    "@lmdb/lmdb-win32-x64": "2.6.0-alpha6"
  },
  "devDependencies": {
    "@types/three": "^0.143.1",
    "parcel": "^2.7.0"
  },
  "dependencies": {
    "three": "^0.143.0"
  }
}
